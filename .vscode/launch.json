{
  // 使用 IntelliSense 了解相关属性。 
  // 悬停以查看现有属性的描述。
  // 欲了解更多信息，请访问: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Program 的测试test.js",
      "program": "${workspaceFolder}/node/node-debug/test.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/node/node-debug/.env",
      "env": {
        "name": "zhaozilong",
      },
      "args": [
        "zhangsan",
        "lisi"
      ],
      "type": "node"
    },
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Attach",
      "port": 9229,
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "node"
    },
    {
      "name": "Launch Program",
      "program": "${workspaceFolder}/node/node-debug/index.js",
      "request": "launch",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "stopOnEntry": true,
      "type": "node"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "针对 test.js进行调试",
      "console": "integratedTerminal",
      "program": "${workspaceFolder}/node/cli-dashboard-test/test.js"
    }
  ]
}